pipeline {
    agent any

    stages {
        stage('Cloning') {
            steps {
                git url: 'https://github.com/clem-kay/learning-jenkins-3003221.git', branch: 'main'
               
            }
        }
          stage('Building Image') {
            steps {
               sh 'docker build -t clem/clem_webserver:latest .'
               
            }
        }
    }
}